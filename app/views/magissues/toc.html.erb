<% content_for :head do %>

<title><%= @magissue.magazine.name %> | TOC</title>
    <%= stylesheet_link_tag(
	"templates/#{@magissue.magtemplate.name.downcase}/#{@magissue.magtemplate.path}") %>
    <link rel="resources prefetch" href="/resources.html"/>
    <link rel="index" href="<%= url_for(content_index_magissue_path(@magissue)) %>" type="application/json"/>
    <%= javascript_include_tag("treesaver-0.10.0-debug") %>
    <!-- FaceBook like -->
    <%= javascript_include_tag("/fb_like") %>
    <!-- end -->
    <!-- LinkedIn share -->
    <script src="//platform.linkedin.com/in.js" type="text/javascript"></script>
    <!-- end -->
    <!-- Twitter -->
    <%= javascript_include_tag("/twitter") %>
    <!-- end -->

<% end %>

<% content_for :treesaver do %>
    <article data-grids="toc">

      <h1>Table Of Contents</h1>
      <% @magissue.articles.each do |article| %>
          <div class="toc-link">
            <h3 ><%= link_to article.title, read_article_path(article), { :magissue => @magissue} %></h3>
            <!--p itemprop="byline"><%#= article.author 
			%></p -->
          </div>
      <% end %>

    </article>


<% end %>